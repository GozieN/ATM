<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase2.UserInterface.AccountMenus.CreateNewBankAccountMenuController">
	<children>
		<Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="[create new bank account menu]">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
		<Label layoutX="38.0" layoutY="99.0" text="select the bank account type to create for this user">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<ComboBox fx:id="bankAccountTypes" layoutX="38.0" layoutY="129.0" prefWidth="150.0" promptText="bank account type" />
		<Label fx:id="bankAccountTypesStatus" layoutX="248.0" layoutY="133.0" />
		<Button layoutX="191.0" layoutY="129.0" mnemonicParsing="false" onAction="#createNewBankAccount" text="submit" />
		<Label fx:id="endStatus" layoutX="248.0" layoutY="133.0" />
		<Button layoutX="14.0" layoutY="542.0" mnemonicParsing="false" onAction="#back" text="back">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
		<Button layoutX="92.0" layoutY="542.0" mnemonicParsing="false" onAction="#exit" text="exit">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
