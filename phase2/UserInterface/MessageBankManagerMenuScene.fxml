<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase2.UserInterface.MessageBankManagerMenuController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="[message bank manager menu]">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TextField fx:id="messageIn" layoutX="92.0" layoutY="86.0" onAction="#sendMessage" promptText="message">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button layoutX="346.0" layoutY="86.0" mnemonicParsing="false" onAction="#sendMessage" text="send message">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="messageInStatus" layoutX="504.0" layoutY="93.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="16.0" layoutY="542.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="92.0" layoutY="542.0" mnemonicParsing="false" onAction="#exit" text="exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
