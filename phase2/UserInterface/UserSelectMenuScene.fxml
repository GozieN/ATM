<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase2.UserInterface.UserSelectMenuController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="[user select menu]">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label layoutX="79.0" layoutY="133.0" text="enter the username of the user to select">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="userUsernameIn" layoutX="79.0" layoutY="163.0" onAction="#selectUser" promptText="username of user">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Label fx:id="userUsernameInStatus" layoutX="334.0" layoutY="170.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="79.0" layoutY="248.0" text="enter your master access key">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="masterAccessKeyIn" layoutX="79.0" layoutY="278.0" onAction="#selectUser" promptText="master access key">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Label fx:id="masterAccessKeyInStatus" layoutX="333.0" layoutY="284.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="255.0" layoutY="374.0" mnemonicParsing="false" onAction="#selectUser" text="select user">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="542.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="91.0" layoutY="542.0" mnemonicParsing="false" onAction="#exit" text="exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
