<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase2.UserInterface.DepositCashMenuController">
	<children>
		<Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="[deposit cash menu]">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
		<Label layoutX="51.0" layoutY="100.0" text="select an account to deposit to">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<ComboBox fx:id="userBankAccounts" layoutX="51.0" layoutY="128.0" prefWidth="150.0" promptText="bank account" />
		<Label fx:id="userBankAccountsStatus" layoutX="206.0" layoutY="132.0" />
		<Label layoutX="52.0" layoutY="221.0" text="enter an amount to deposit (multiples of 5 only)">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<TextField fx:id="amount" layoutX="52.0" layoutY="251.0" onAction="#deposit" promptText="amount" />
		<Label fx:id="amountStatus" layoutX="206.0" layoutY="255.0" />
		<Button layoutX="126.0" layoutY="351.0" mnemonicParsing="false" onAction="#deposit" text="deposit">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
		<Label fx:id="endStatus" layoutX="226.0" layoutY="358.0">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<Button layoutX="14.0" layoutY="542.0" mnemonicParsing="false" onAction="#back" text="back">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
		<Button layoutX="90.0" layoutY="542.0" mnemonicParsing="false" onAction="#exit" text="exit">
			<font>
				<Font size="20.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
