<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phase2.UserInterface.SetAtmDateMenuController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="[set ATM date menu]">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TextField fx:id="dayIn" layoutX="121.0" layoutY="115.0" onAction="#setDate" promptText="day">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Label fx:id="dayInStatus" layoutX="377.0" layoutY="122.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="monthIn" layoutX="121.0" layoutY="166.0" onAction="#setDate" promptText="month">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Label fx:id="monthInStatus" layoutX="376.0" layoutY="173.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="yearIn" layoutX="121.0" layoutY="217.0" onAction="#setDate" promptText="year">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Label fx:id="yearInStatus" layoutX="376.0" layoutY="224.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="271.0" layoutY="268.0" mnemonicParsing="false" onAction="#setDate" text="set date">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="endStatus" layoutX="376.0" layoutY="275.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="542.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="88.0" layoutY="542.0" mnemonicParsing="false" onAction="#exit" text="exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
